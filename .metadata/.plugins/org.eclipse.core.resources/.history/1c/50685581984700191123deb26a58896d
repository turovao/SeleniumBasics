package com.review4;

public class test {
	public static void main(String[] args) {
		
		
//		Create a do while loop that prints out numbers from 10 to 20
//
//		Must not include the number 20 in the output 
//
//		Increment by 1 within the do example:
//		do  {
//		a++;
//		 }
	
		int a=10;
		do {
			System.out.println(a);
			a++;
		}while(a<20);
	
	
	
	
	
	}

}
