package com.class15AnswerForInterviewQuestions;

public class Task7 {
	public static void main(String[] args) {
		
//		*
//        * Find out how many alpha characters present in a string?
//        */

   String str = "123We are the best456%%";
   String newstr = str.replaceAll("[^A-z]", "");
   
   System.out.println(newstr.length());
   
   
   
   String main="777 Lucky numbers123*&^%";
   System.out.println(main.replaceAll("[^A-Za-z]", "").length());
		
	}

}
