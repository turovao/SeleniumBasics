package com.review4;

public class test {
	public static void main(String[] args) {
		
		
//		Create a do while loop that prints out numbers from 7 to 15
//
//		Must  include the number 39 in the output 
//
//		Increment by 1 within the do example:
//		do  {
//		a--;
//		 }
	
		int s=15;
		while(s>=7) {
			System.out.println(s);
			s--;
		}
		
	}

}
