package com.review4;


// Write a program that the String "the time is 07:min
// and then all the mins till 7:30
public class Task {
	public static void main(String[] args) {
		
	
		
	for(int i=0; i<=30; i++) {
		
			System.out.println("The time is 07:"+i);	
	}

   int min=00;
   
   while(min<=30) {
	   
	   System.out.println("The times is 07:0"+min);

   min++;
   }
   
   
   
   int minz=100;
   
   do {
	   
System.out.println("The tim is 07:0"+minz);

minz--;

   }
   
   while(minz>=30);
   
   
	
   
   }
   }
   















